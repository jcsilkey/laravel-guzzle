{
  "name": "jcsilkey/laravel-guzzle",
  "description": "Guzzle Service Provider for Laravel",
  "type": "library",
  "license": "MIT",
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "sort-packages": true,
    "bin-dir": "bin"
  },
  "require": {
    "php": ">=7.2.0",
    "guzzlehttp/guzzle": "^6.3",
    "illuminate/support": "^5.6"
  },
  "require-dev": {
    "orchestra/testbench": "^3.6",
    "phpunit/phpunit": "^7.1"
  },
  "autoload": {
    "psr-4": {
      "JCS\\LaravelGuzzle\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "JCS\\LaravelGuzzle\\": "tests/"
    }
  },
  "extras": {
    "laravel": {
      "providers": [
        "JCS\\LaravelGuzzle\\GuzzleServiceProvider"
      ]
    }
  }
}
